<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Subscription_Status__c</fullName>
    <description>Field ini memberikan status otomatis pada subscription berdasarkan jarak waktu dari Start Date</description>
    <externalId>false</externalId>
    <formula>IF(TODAY() - Start_Date__c &gt; 30, &quot;Active&quot;, &quot;Pending&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Field must be filled</inlineHelpText>
    <label>Subscription Status</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
