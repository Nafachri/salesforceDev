<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Registered_Count_Limit</fullName>
    <active>true</active>
    <errorConditionFormula>Registered_Count__c &gt;= Max_Capacity__c</errorConditionFormula>
    <errorMessage>Webinar is already full booked</errorMessage>
</ValidationRule>
